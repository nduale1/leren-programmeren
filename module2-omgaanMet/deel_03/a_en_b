GetalA= int (input ('Aan welke cijfer denkt u voor getalA?'))
GetalB= int (input ('Aan welke cijfer denkt u voor getalB?'))
if  GetalA > GetalB: 
 print("A is het grootste getal")
 print (f'{max}')
 max= GetalA  

print ('-------')

GetalC= int (input ('Aan welke cijfer denkt u voor getalC?:'))
GetalD= int (input ('Aan welke cijfer denkt u voor getalD?:'))
elif GetalC < GetalD:
 print ("GetalC is het kleinste getal")
 print (f'{min}')
 min= GetalC 


GetalE= int (input ('Aan welke cijfer denkt u voor getalE?:'))
GetalF= int (input ('Aan welke cijfer denkt u voor getalF?:'))
else=  GetalE <= GetalF: 
print  ('getalE en getalF zijn even groot')  

print (f'Het minimum is: {min}')
print (f'Het maximum is: {max}')
